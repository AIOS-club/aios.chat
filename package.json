{
  "name": "aios.chat",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "inst": "npm i -g pnpm && pnpm i && cd ./api && pnpm i && cd ..",
    "dev:env": "node entry.js && npm run dev",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:express\"",
    "dev:vite": "vite",
    "dev:express": "cross-env NODE_ENV=development nodemon --exec esno watch api/src/app.ts",
    "build": "npm run build:vite && npm run build:express",
    "build:vite": "tsc && vite build",
    "build:express": "cross-env NODE_ENV=production tsup api/src/app.ts --out-dir dist/ --format cjs",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --max-warnings 999"
  },
  "dependencies": {
    "@andylacko/vite-svg-react-loader": "^3.0.3",
    "@douyinfe/semi-icons": "^2.31.0",
    "@douyinfe/semi-ui": "^2.25.0",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@react-spring/shared": "^9.7.2",
    "@react-spring/web": "^9.7.2",
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-legacy": "^4.0.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "cross-env": "^7.0.3",
    "emoji-mart": "^5.5.2",
    "highlight.js": "^11.7.0",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.4",
    "less": "^4.1.3",
    "marked": "^4.2.12",
    "marked-katex": "^0.3.8",
    "marked-katex-extension-ts": "^0.1.2",
    "moment": "^2.29.4",
    "nodemon": "^2.0.21",
    "photoswipe": "^5.3.6",
    "postcss": "^8.4.20",
    "prop-types": "^15.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.3",
    "unplugin-auto-import": "^0.12.1",
    "uuid": "^9.0.0",
    "vite-plugin-pages": "^0.28.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/emoji-mart": "^3.0.9",
    "@types/katex": "^0.16.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "esno": "^0.16.3",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  }
}
